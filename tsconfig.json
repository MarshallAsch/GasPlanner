{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "projects/planner/types"
    ],
    "types": [
      "projects/planner/types/plotly.js/index.d.ts"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "scuba-physics": [
        "dist/scuba-physics"
      ],
      "scuba-physics/*": [
        "dist/scuba-physics/*"
      ]
    }
  }
}
